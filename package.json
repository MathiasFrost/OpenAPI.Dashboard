{
	"name": "openapi-dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "env-cmd -e root,development vite dev",
		"beta": "env-cmd -e root,beta vite dev",
		"release": "env-cmd -e root,release vite dev",
		"build": "env-cmd -e root,release vite build",
		"build:beta": "env-cmd -e root,beta vite build",
		"preview": "vite preview",
		"test": "playwright install && playwright test",
		"test:debug": "playwright test --debug",
		"test:open": "playwright show-report",
		"test:trace": "playwright show-trace",
		"check": "env-cmd -e root,beta svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "env-cmd -e root,beta svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:css": "stylelint **/*.{css,scss,html,svelte,svg}",
		"format": "prettier --plugin-search-dir . --write .",
		"inspect": "pnpm check && pnpm lint && pnpm lint:css"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.15.6",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.22",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.0",
		"stylelint": "^15.5.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^8.0.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	},
	"type": "module",
	"dependencies": {
		"@maal/svelte-data": "file:..\\Svelte.Data\\maal-svelte-data-0.1.1.tgz"
	}
}
